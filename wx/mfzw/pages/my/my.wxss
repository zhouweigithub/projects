.headView {
  text-align: center;
}

.headView button {
  margin: 50px auto;
  /* background-color: white; */
  border: solid thin #248311;
  background: transparent;
}

.headView button::after {
  border: none;
}

.headView image {
  width: 100px;
  height: 100px;
  margin: 20px auto 30px auto;
  display: block;
}

.headView text {
  font-size: 25px;
  font-weight: bold;
}

.countView {
  margin: 30px auto;
  /* border-top: solid thin gray; *//* background-color: #c1d1bd; */
}

.countView view {
  padding: 30px auto;
  width: 40%;
  height: 60px;
  line-height: 60px;
  display: inline-block;
  font-size: 20px;
  border-bottom: solid thin #248311;
  border-top: solid thin #248311;
}

.favView {
  border-right: solid thin #248311;
}

.listView {
  width: 80%;
  margin: 30px auto;
  font-size: 17px;
  line-height: 40px;
  text-align: left;
}

.listView view {
  border-bottom: solid thin #248311;
}

.feedbackView .char {
  float: right;
}

.backToHome {
  display: inline-block;
  margin-top: 50px;
  padding: 10px;
  border: solid thin #248311;
  border-radius: 5px;
  /* text-decoration: underline; */
}
